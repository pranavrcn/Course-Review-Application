<?xml version="1.0" encoding="UTF-8"?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml"
            fx:controller="edu.virginia.sde.reviews.MyReviewsController"
            prefHeight="720.0" prefWidth="1280.0">
    <children>
        <Button text="Back" onAction="#handleBackButton"/>

        <TableView fx:id="reviewsTable" prefHeight="600.0" prefWidth="1280.0" AnchorPane.bottomAnchor="50.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="50.0">
            <columns>
                <TableColumn fx:id="mnemonicColumn" text="Mnemonic" prefWidth="300.0"/>
                <TableColumn fx:id="numberColumn" text="Number" prefWidth="300.0"/>
                <TableColumn fx:id="titleColumn" text="Title" prefWidth="120.0"/>
                <TableColumn fx:id="ratingColumn" text="Rating" prefWidth="120.0"/>
                <TableColumn fx:id="commentColumn" text="Comment" prefWidth="120.0"/>
            </columns>
        </TableView>
    </children>
</AnchorPane>